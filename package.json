{
  "name": "@itwin/property-validation-client",
  "version": "0.1.0",
  "description": "Property Validation client for the iTwin platform",
  "main": "lib/cjs/property-validation-client.js",
  "module": "lib/esm/property-validation-client.js",
  "typings": "lib/cjs/property-validation-client",
  "scripts": {
    "build": "npm run -s build:cjs && npm run -s build:esm",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "lint": "eslint -f visualstudio \"./src/**/*.ts\" 1>&2",
    "lint:fix": "npm run lint -- --fix",
    "webpackTests": "webpack --config ./src/test/utils/webpack.config.js 1>&2",
    "test": "npm run -s webpackTests && npm run -s test:chrome",
    "test:chrome": "certa -r chrome",
    "test:electron": "certa -r electron",
    "pack": "npm pack",
    "clean": "rimraf lib"
  },
  "keywords": [
    "Bentley",
    "iTwin",
    "digital-twin",
    "validation",
    "IModelHub"
  ],
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/property-validation-client"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@itwin/build-tools": "^3.0.0",
    "@itwin/certa": "^3.0.0",
    "@itwin/core-bentley": "^3.0.0",
    "@itwin/eslint-plugin": "^3.0.0",
    "@itwin/oidc-signin-tool": "^3.2.0",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.14.31",
    "chai": "^4.1.2",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^7.11.0",
    "mocha": "^8.3.2",
    "openid-client": "^3.15.3",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.0.0",
    "typescript": "~4.4.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.0"
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": "plugin:@itwin/itwinjs-recommended"
  }
}
